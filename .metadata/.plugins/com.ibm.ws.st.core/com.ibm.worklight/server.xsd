                <xsd:appinfo> 
                    <ext:label>Nome de Servidores de Failover LDAP</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:anyAttribute processContents="skip"> 
        </xsd:anyAttribute> 
    </xsd:complexType> 
    <xsd:complexType name="com.ibm.ws.security.registry.ldap.config.failoverServers-factory"> 
