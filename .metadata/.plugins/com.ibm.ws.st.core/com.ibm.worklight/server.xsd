            <xsd:documentation>Propriedades de configuração para o registro de usuários LDAP.</xsd:documentation> 
            <xsd:appinfo> 
                <ext:label>Registro do Usuário LDAP</ext:label> 
            </xsd:appinfo> 
        </xsd:annotation> 
        <xsd:choice minOccurs="0" maxOccurs="unbounded"> 
            <xsd:element name="failoverServers" type="com.ibm.ws.security.registry.ldap.config.failoverServers"> 
                <xsd:annotation> 
                    <xsd:documentation>Lista de servidores de failover LDAP.</xsd:documentation> 
                    <xsd:appinfo> 
                        <ext:label>Servidores de Failover LDAP</ext:label> 
                    </xsd:appinfo> 
                </xsd:annotation> 
            </xsd:element> 
            <xsd:element name="activedFilters" type="com.ibm.ws.security.registry.ldap.internal.filters.actived"> 
                <xsd:annotation> 
                    <xsd:documentation>Especifica a lista de filtros LDAP padrão do Microsoft Active Directory.</xsd:documentation> 
                    <xsd:appinfo> 
                        <ext:label>Filtros LDAP do Microsoft Active Directory</ext:label> 
                    </xsd:appinfo> 
                </xsd:annotation> 
            </xsd:element> 
            <xsd:element name="customFilters" type="com.ibm.ws.security.registry.ldap.internal.filters.custom"> 
                <xsd:annotation> 
                    <xsd:documentation>Especifica a lista de filtros LDAP Customizados padrão.</xsd:documentation> 
                    <xsd:appinfo> 
                        <ext:label>Filtros LDAP Customizados</ext:label> 
                    </xsd:appinfo> 
                </xsd:annotation> 
            </xsd:element> 
            <xsd:element name="domino50Filters" type="com.ibm.ws.security.registry.ldap.internal.filters.domino50"> 
                <xsd:annotation> 
                    <xsd:documentation>Especifica a lista de filtros LDAP padrão do IBM Lotus Domino.</xsd:documentation> 
                    <xsd:appinfo> 
                        <ext:label>Filtros LDAP do IBM Lotus Domino</ext:label> 
                    </xsd:appinfo> 
                </xsd:annotation> 
            </xsd:element> 
            <xsd:element name="edirectoryFilters" type="com.ibm.ws.security.registry.ldap.internal.filters.eDirectory"> 
                <xsd:annotation> 
                    <xsd:documentation>Especifica a lista de filtros LDAP do Novell eDirectory.</xsd:documentation> 
                    <xsd:appinfo> 
                        <ext:label>Filtros LDAP do Novell eDirectory</ext:label> 
                    </xsd:appinfo> 
                </xsd:annotation> 
            </xsd:element> 
            <xsd:element name="idsFilters" type="com.ibm.ws.security.registry.ldap.internal.filters.ids"> 
                <xsd:annotation> 
                    <xsd:documentation>Especifica a lista de filtros LDAP padrão do IBM Tivoli Directory Server.</xsd:documentation> 
                    <xsd:appinfo> 
                        <ext:label>Filtros LDAP do IBM Tivoli Directory Server</ext:label> 
                    </xsd:appinfo> 
                </xsd:annotation> 
            </xsd:element> 
            <xsd:element name="iplanetFilters" type="com.ibm.ws.security.registry.ldap.internal.filters.iPlanet"> 
                <xsd:annotation> 
                    <xsd:documentation>Especifica a lista de filtros LDAP padrão do Sun Java System Directory Server.</xsd:documentation> 
                    <xsd:appinfo> 
                        <ext:label>Filtros LDAP do Sun Java System Directory Server</ext:label> 
                    </xsd:appinfo> 
                </xsd:annotation> 
            </xsd:element> 
            <xsd:element name="netscapeFilters" type="com.ibm.ws.security.registry.ldap.internal.filters.netscape"> 
                <xsd:annotation> 
                    <xsd:documentation>Especifica a lista de filtros LDAP padrão do Netscape Directory Server.</xsd:documentation> 
                    <xsd:appinfo> 
                        <ext:label>Filtros LDAP do Netscape Directory Server</ext:label> 
                    </xsd:appinfo> 
                </xsd:annotation> 
            </xsd:element> 
            <xsd:element name="securewayFilters" type="com.ibm.ws.security.registry.ldap.internal.filters.secureway"> 
                <xsd:annotation> 
                    <xsd:documentation>Especifica a lista de filtros LDAP padrão do IBM SecureWay Directory Server.</xsd:documentation> 
                    <xsd:appinfo> 
                        <ext:label>Filtros LDAP do IBM SecureWay Directory Server</ext:label> 
                    </xsd:appinfo> 
                </xsd:annotation> 
            </xsd:element> 
            <xsd:element name="contextPool" type="com.ibm.ws.security.wim.adapter.ldap.contextPool"> 
                <xsd:annotation> 
                    <xsd:documentation>Propriedades do conjunto de contexto.</xsd:documentation> 
                    <xsd:appinfo> 
                        <ext:label>Propriedades do Conjunto de Contexto</ext:label> 
                    </xsd:appinfo> 
                </xsd:annotation> 
            </xsd:element> 
            <xsd:element name="ldapCache" type="com.ibm.ws.security.wim.adapter.ldap.cacheConfig"> 
                <xsd:annotation> 
                    <xsd:documentation>Configure os atributos do cache.</xsd:documentation> 
                    <xsd:appinfo> 
                        <ext:label>Propriedades do Cache</ext:label> 
                    </xsd:appinfo> 
                </xsd:annotation> 
            </xsd:element> 
        </xsd:choice> 
        <xsd:attribute name="host" use="required" type="xsd:string"> 
            <xsd:annotation> 
                <xsd:documentation>Endereço do servidor LDAP na forma de um endereço IP ou de um nome do serviço de domínio de nomes (DNS).</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>Host</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="port" use="required" type="intType"> 
            <xsd:annotation> 
                <xsd:documentation>Número da porta do servidor LDAP.</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>Porta</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="baseDN" use="required" type="xsd:string"> 
            <xsd:annotation> 
                <xsd:documentation>Nome distinto (DN) de base do serviço de diretório, que indica o ponto de início para procuras LDAP no serviço de diretório.</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>Nome Distinto de Base (dn)</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="ldapType" use="required"> 
            <xsd:annotation> 
                <xsd:documentation>Tipo de servidor LDAP com o qual uma conexão será estabelecida.</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>Tipo de Servidor LDAP</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
            <xsd:simpleType> 
                <xsd:restriction base="xsd:string"> 
                    <xsd:enumeration value="Microsoft Active Directory"> 
                        <xsd:annotation> 
                            <xsd:documentation>actived</xsd:documentation> 
                        </xsd:annotation> 
                    </xsd:enumeration> 
                    <xsd:enumeration value="Custom"> 
                        <xsd:annotation> 
                            <xsd:documentation>custom</xsd:documentation> 
                        </xsd:annotation> 
                    </xsd:enumeration> 
                    <xsd:enumeration value="IBM Lotus Domino"> 
                        <xsd:annotation> 
                            <xsd:documentation>domino50</xsd:documentation> 
                        </xsd:annotation> 
                    </xsd:enumeration> 
                    <xsd:enumeration value="Novell eDirectory"> 
                        <xsd:annotation> 
                            <xsd:documentation>edirectory</xsd:documentation> 
                        </xsd:annotation> 
                    </xsd:enumeration> 
                    <xsd:enumeration value="IBM Tivoli Directory Server"> 
                        <xsd:annotation> 
                            <xsd:documentation>ibm_dir_server</xsd:documentation> 
                        </xsd:annotation> 
                    </xsd:enumeration> 
                    <xsd:enumeration value="Sun Java System Directory Server"> 
                        <xsd:annotation> 
                            <xsd:documentation>iplanet</xsd:documentation> 
                        </xsd:annotation> 
                    </xsd:enumeration> 
                    <xsd:enumeration value="Netscape Directory Server"> 
                        <xsd:annotation> 
                            <xsd:documentation>netscape</xsd:documentation> 
                        </xsd:annotation> 
                    </xsd:enumeration> 
                    <xsd:enumeration value="IBM SecureWay Directory Server"> 
                        <xsd:annotation> 
                            <xsd:documentation>secureway</xsd:documentation> 
                        </xsd:annotation> 
                    </xsd:enumeration> 
                </xsd:restriction> 
            </xsd:simpleType> 
        </xsd:attribute> 
        <xsd:attribute name="realm" use="optional" default="LdapRegistry" type="xsd:string"> 
            <xsd:annotation> 
                <xsd:documentation>O nome da região que representa o registro do usuário.</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>Nome da região</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="bindDN" use="optional" type="xsd:string"> 
            <xsd:annotation> 
                <xsd:documentation>Nome distinto (DN) para o servidor de aplicativos, que é usado para ligar ao serviço de diretório.</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>Nome Distinto da Ligação (dn)</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="bindPassword" use="optional" type="password"> 
            <xsd:annotation> 
                <xsd:documentation>Senha para o DN de ligação. O valor pode ser armazenado no formato de texto não criptografado ou codificado. É recomendado que você codifique a senha. Para fazer isso, use a  ferramenta securityUtility com a opção de codificação.</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>Senha de Ligação</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="ignoreCase" use="optional" default="true" type="booleanType"> 
            <xsd:annotation> 
                <xsd:documentation>Execute uma verificação de autenticação sem distinção entre maiúsculas e minúsculas.</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>Ignorar Distinção entre Maiúsculas e Minúsculas para Autorização</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="recursiveSearch" use="optional" default="false" type="booleanType"> 
            <xsd:annotation> 
                <xsd:documentation>Executa uma procura de grupo aninhado. Selecione esta opção apenas se o servidor LDAP não suportar procuras recursivas do lado do servidor.</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>Executar uma Procura de Grupo Aninhado</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="reuseConnection" use="optional" default="true" type="booleanType"> 
            <xsd:annotation> 
                <xsd:documentation>Solicita ao servidor de aplicativos para reutilizar a conexão do servidor LDAP.</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>Reutilizar Conexão</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="sslEnabled" use="optional" default="false" type="booleanType"> 
            <xsd:annotation> 
                <xsd:documentation>Indica se uma conexão SSL deve ser feita no servidor LDAP.</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>LDAP SSL Ativado</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="sslRef" use="optional" type="xsd:string"> 
            <xsd:annotation> 
                <xsd:documentation>ID da configuração SSL a ser usada para conectar-se ao servidor LDAP ativado por SSL.</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>Referência SSL</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="searchTimeout" use="optional" default="1m" type="duration"> 
            <xsd:annotation> 
                <xsd:documentation>Tempo máximo para um servidor LDAP responder antes de uma solicitação ser cancelada.  Isso é equivalente a um tempo limite de leitura depois que a conexão é estabelecida. Especifique um número inteiro positivo seguido por uma unidade de tempo, que pode ser horas (h), minutos (m), segundos (s) ou milissegundos (ms). Por exemplo, especifique 500 milissegundos como 500 ms. É possível incluir diversos valores em uma única entrada. Por exemplo, 1s500ms é equivalente a 1,5 segundos.</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>Tempo limite de pesquisa</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="connectTimeout" use="optional" default="1m" type="duration"> 
            <xsd:annotation> 
                <xsd:documentation>O tempo máximo para estabelecer uma conexão com o servidor LDAP. Uma mensagem de erro será registrada em log se o tempo especificado expirar. Especifique um número inteiro positivo seguido por uma unidade de tempo, que pode ser horas (h), minutos (m), segundos (s) ou milissegundos (ms). Por exemplo, especifique 500 milissegundos como 500 ms. É possível incluir diversos valores em uma única entrada. Por exemplo, 1s500ms é equivalente a 1,5 segundos.</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>A duração do tempo limite para estabelecer conexão LDAP</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="certificateMapMode" use="optional"> 
            <xsd:annotation> 
                <xsd:documentation>Especifica se os certificados x.509 devem ser mapeados para um diretório LDAP por EXACT_DN ou CERTIFICATE_FILTER. Especifique CERTIFICATE_FILTER para utilizar o filtro de certificado especificado para o mapeamento.</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>Modo do Mapa de Certificado</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
            <xsd:simpleType> 
                <xsd:restriction base="xsd:string"> 
                    <xsd:enumeration value="EXACT_DN"> 
                        <xsd:annotation> 
                            <xsd:documentation>exactDN</xsd:documentation> 
                        </xsd:annotation> 
                    </xsd:enumeration> 
                    <xsd:enumeration value="CERTIFICATE_FILTER"> 
                        <xsd:annotation> 
                            <xsd:documentation>certFilter</xsd:documentation> 
                        </xsd:annotation> 
                    </xsd:enumeration> 
                </xsd:restriction> 
            </xsd:simpleType> 
        </xsd:attribute> 
        <xsd:attribute name="certificateFilter" use="optional" type="xsd:string"> 
            <xsd:annotation> 
                <xsd:documentation>Especifica a propriedade de mapeamento de certificado de filtro para o filtro LDAP. O filtro é usado para mapear atributos no certificado de cliente para entradas no registro LDAP. Por exemplo, o filtro pode ser especificado como: uid=${SubjectCN}.</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>Filtro de mapa de certificado</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="activedFiltersRef" use="optional" type="pidType"> 
            <xsd:annotation> 
                <xsd:documentation>Especifica a lista de filtros LDAP padrão do Microsoft Active Directory.</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>Referência de filtros LDAP do Microsoft Active Directory</ext:label> 
                    <ext:reference>activedLdapFilterProperties</ext:reference> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="customFiltersRef" use="optional" type="pidType"> 
            <xsd:annotation> 
                <xsd:documentation>Especifica a lista de filtros LDAP Customizados padrão.</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>Referência de Filtros LDAP Customizados</ext:label> 
                    <ext:reference>customLdapFilterProperties</ext:reference> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="domino50FiltersRef" use="optional" type="pidType"> 
            <xsd:annotation> 
                <xsd:documentation>Especifica a lista de filtros LDAP padrão do IBM Lotus Domino.</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>Referência de filtros LDAP do IBM Lotus Domino</ext:label> 
                    <ext:reference>domino50LdapFilterProperties</ext:reference> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="edirectoryFiltersRef" use="optional" type="pidType"> 
            <xsd:annotation> 
                <xsd:documentation>Especifica a lista de filtros LDAP do Novell eDirectory.</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>Referência de Filtros LDAP do Novell eDirectory</ext:label> 
                    <ext:reference>edirectoryLdapFilterProperties</ext:reference> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="idsFiltersRef" use="optional" type="pidType"> 
            <xsd:annotation> 
                <xsd:documentation>Especifica a lista de filtros LDAP padrão do IBM Tivoli Directory Server.</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>Referência de filtros LDAP do IBM Tivoli Directory</ext:label> 
                    <ext:reference>idsLdapFilterProperties</ext:reference> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="iplanetFiltersRef" use="optional" type="pidType"> 
            <xsd:annotation> 
                <xsd:documentation>Especifica a lista de filtros LDAP padrão do Sun Java System Directory Server.</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>Referência de filtros LDAP do Sun Java System Directory</ext:label> 
                    <ext:reference>iplanetLdapFilterProperties</ext:reference> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="netscapeFiltersRef" use="optional" type="pidType"> 
            <xsd:annotation> 
                <xsd:documentation>Especifica a lista de filtros LDAP padrão do Netscape Directory Server.</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>Referência de filtros LDAP do Netscape Directory Server</ext:label> 
                    <ext:reference>netscapeLdapFilterProperties</ext:reference> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="securewayFiltersRef" use="optional" type="pidType"> 
            <xsd:annotation> 
                <xsd:documentation>Especifica a lista de filtros LDAP padrão do IBM SecureWay Directory Server.</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>Referência de filtros LDAP do IBM SecureWay Directory</ext:label> 
                    <ext:reference>securewayLdapFilterProperties</ext:reference> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="returnToPrimaryServer" use="optional" default="true" type="booleanType"> 
            <xsd:annotation> 
                <xsd:documentation>Um valor booleano que indica se a procura deve ser feita no Servidor Principal.</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>Retornar para o Servidor Principal</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:anyAttribute processContents="skip"> 
        </xsd:anyAttribute> 
    </xsd:complexType> 
    <xsd:complexType name="com.ibm.ws.security.registry.ldap.config-factory"> 
        <xsd:complexContent> 
            <xsd:extension base="com.ibm.ws.security.registry.ldap.config"> 
                <xsd:attribute name="id" type="factoryIdType" use="optional"> 
                    <xsd:annotation> 
                        <xsd:documentation>Um ID de configuração exclusivo.</xsd:documentation> 
                        <xsd:appinfo> 
                            <ext:label>ID</ext:label> 
                        </xsd:appinfo> 
                    </xsd:annotation> 
                </xsd:attribute> 
            </xsd:extension> 
        </xsd:complexContent> 
    </xsd:complexType> 
    <xsd:complexType name="com.ibm.ws.security.registry.ldap.internal.filters.secureway"> 
        <xsd:annotation> 
            <xsd:documentation>Especifica a lista de filtros LDAP padrão do IBM SecureWay Directory Server.</xsd:documentation> 
            <xsd:appinfo> 
                <ext:label>Filtros LDAP do IBM SecureWay Directory Server</ext:label> 
            </xsd:appinfo> 
        </xsd:annotation><!--Idiom for "empty element context" (whitespace accepted and ignored)--> 
        <xsd:sequence> 
            <xsd:sequence/> 
        </xsd:sequence> 
        <xsd:attribute name="userFilter" use="optional" default="(&amp;(uid=%v)(objectclass=ePerson))" type="xsd:string"> 
            <xsd:annotation> 
                <xsd:documentation>Uma cláusula de filtro LDAP para procurar o registro de usuário para os usuários.</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>Filtro de Usuário</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="groupFilter" use="optional" default="(&amp;(cn=%v)(|(objectclass=groupOfNames)(objectclass=groupOfUniqueNames)))" type="xsd:string"> 
            <xsd:annotation> 
                <xsd:documentation>Uma cláusula de filtro LDAP para procurar o registro do usuário para os grupos.</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>Filtro de grupo</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="userIdMap" use="optional" default="*:uid" type="xsd:string"> 
            <xsd:annotation> 
                <xsd:documentation>Um filtro LDAP que mapeia o nome de um usuário para uma entrada de LDAP.</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>Mapa do ID do Usuário</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="groupIdMap" use="optional" default="*:cn" type="xsd:string"> 
            <xsd:annotation> 
                <xsd:documentation>Um filtro LDAP que mapeia o nome de um grupo para uma entrada LDAP.</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>Mapa do ID do Grupo</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="groupMemberIdMap" use="optional" default="groupOfNames:member;groupOfUniqueNames:uniqueMember" type="xsd:string"> 
            <xsd:annotation> 
                <xsd:documentation>Um filtro LDAP que identifica o usuário para as associações ao grupo.</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>Mapa do ID do Membro do Grupo</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:anyAttribute processContents="skip"> 
        </xsd:anyAttribute> 
    </xsd:complexType> 
    <xsd:complexType name="com.ibm.ws.security.registry.ldap.internal.filters.secureway-factory"> 
        <xsd:complexContent> 
            <xsd:extension base="com.ibm.ws.security.registry.ldap.internal.filters.secureway"> 
                <xsd:attribute name="id" type="factoryIdType" use="optional"> 
                    <xsd:annotation> 
                        <xsd:documentation>Um ID de configuração exclusivo.</xsd:documentation> 
                        <xsd:appinfo> 
                            <ext:label>ID</ext:label> 
                        </xsd:appinfo> 
                    </xsd:annotation> 
                </xsd:attribute> 
            </xsd:extension> 
        </xsd:complexContent> 
    </xsd:complexType> 
    <xsd:complexType name="com.ibm.ws.security.wim.adapter.ldap.dynamicMemberAttribute"> 
        <xsd:annotation> 
            <xsd:documentation>A configuração para o atributo de membro dinâmico LDAP.</xsd:documentation> 
            <xsd:appinfo> 
                <ext:label>Atributo de Membro Dinâmico LDAP</ext:label> 
            </xsd:appinfo> 
        </xsd:annotation><!--Idiom for "empty element context" (whitespace accepted and ignored)--> 
        <xsd:sequence> 
            <xsd:sequence/> 
        </xsd:sequence> 
        <xsd:attribute name="name" use="optional" type="xsd:string"> 
            <xsd:annotation> 
                <xsd:documentation>O nome do membro.</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>Nome do Membro</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="objectClass" use="optional" type="xsd:string"> 
            <xsd:annotation> 
                <xsd:documentation>O nome da classe de objeto.</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>Classe de Objeto</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:anyAttribute processContents="skip"> 
        </xsd:anyAttribute> 
    </xsd:complexType> 
    <xsd:complexType name="com.ibm.ws.security.wim.adapter.ldap.dynamicMemberAttribute-factory"> 
        <xsd:complexContent> 
            <xsd:extension base="com.ibm.ws.security.wim.adapter.ldap.dynamicMemberAttribute"> 
                <xsd:attribute name="id" type="factoryIdType" use="optional"> 
                    <xsd:annotation> 
                        <xsd:documentation>Um ID de configuração exclusivo.</xsd:documentation> 
                        <xsd:appinfo> 
                            <ext:label>ID</ext:label> 
                        </xsd:appinfo> 
                    </xsd:annotation> 
                </xsd:attribute> 
            </xsd:extension> 
        </xsd:complexContent> 
    </xsd:complexType> 
    <xsd:complexType name="com.ibm.ws.security.registry.ldap.internal.filters.iPlanet"> 
        <xsd:annotation> 
            <xsd:documentation>Especifica a lista de filtros LDAP padrão do Sun Java System Directory Server.</xsd:documentation> 
            <xsd:appinfo> 
                <ext:label>Filtros LDAP do Sun Java System Directory Server</ext:label> 
            </xsd:appinfo> 
        </xsd:annotation><!--Idiom for "empty element context" (whitespace accepted and ignored)--> 
        <xsd:sequence> 
            <xsd:sequence/> 
        </xsd:sequence> 
        <xsd:attribute name="userFilter" use="optional" default="(&amp;(uid=%v)(objectclass=inetOrgPerson))" type="xsd:string"> 
            <xsd:annotation> 
                <xsd:documentation>Uma cláusula de filtro LDAP para procurar o registro de usuário para os usuários.</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>Filtro de Usuário</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
